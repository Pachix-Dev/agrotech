---
import { t, changeLanguage } from "i18next";
import Layout from "../../layouts/Layout.astro";

changeLanguage("en");
---

<Layout title='Agrotech | Conferences program'>
  <div class='my-10 h-full container mx-auto'>
    <h1 class='font-bold text-center text-4xl'>
      {t('program.title')}
    </h1>
    <p class='mt-10 text-center text-3xl'>
      {t('program.description')}
    </p>
  </div>
</Layout>
